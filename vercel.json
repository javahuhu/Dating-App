{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build/web"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/build/web/$1"
    }
  ],
  "buildCommand": "flutter build web",
  "outputDirectory": "build/web",
  "installCommand": "flutter pub get"
}
